{
  "private": true,
  "name": "system1",
  "scripts": {
    "start": "cross-env umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "antd": "^4.17.1",
    "react": "^17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "17.x",
    "umi": "^3.5.20",
    "watermark-component-for-react": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-qiankun": "^2.35.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "cross-env": "^7.0.3",
    "glob": "^7.2.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react-dev-inspector": "^1.7.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}